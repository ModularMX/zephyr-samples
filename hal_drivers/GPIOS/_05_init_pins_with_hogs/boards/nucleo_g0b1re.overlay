/ {
    aliases {
        gpio = &gpioa;
    };
};

&gpioa {
    /* Declare an internal node name "pin" and set pin 5 as output with init 
    state as high level, you can read gpio-controller.yaml file to check the 
    rest of the properties */
    pin {
        gpio-hog;
        gpios = <5 GPIO_ACTIVE_HIGH>;
        output-high;
        line-name = "output pin"; 
    };
};